{
  "database": {
    "host": "{{ msp_pgsql_address }}",
    "port": 5432,
    "schema": "msp_admin_service",
    "database": "{{ msp_pgsql_name }}",
    "username": "{{ msp_pgsql_user }}",
    "password": "{{ msp_pgsql_password }}"
  },
  "logLevel": "debug",
  "antiBruteforce": {
    "maxInFlightLoginRequests" : 3,
    "delayLoginRequestInSec": 3
  },
  "expireSec": 3600
}
